<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<customMultiLayerMapSource>
   <name>天地图 混合地图</name>
   <tileType>png</tileType>
   <layers>
      <customMapSource>
         <name>天地图卫星图层</name>
         <minZoom>3</minZoom>
         <maxZoom>18</maxZoom>
         <tileType>JPG</tileType>
         <ignoreErrors>true</ignoreErrors>
         <backgroundColor>#f5f4ed</backgroundColor>
         <url><![CDATA[http://t{$serverpart}.tianditu.cn/DataServer?T=img_w&X={$x}&Y={$y}&L={$z}]]></url>
         <serverParts>0 1 2 3 5 6 7</serverParts>
       </customMapSource>
      <customMapSource>
         <name>天地图文字层</name>
         <minZoom>3</minZoom>
         <maxZoom>18</maxZoom>
         <tileType>PNG</tileType>
         <ignoreErrors>true</ignoreErrors>
         <url><![CDATA[http://t{$serverpart}.tianditu.cn/DataServer?T=cva_w&X={$x}&Y={$y}&L={$z}]]></url>
         <serverParts>0 1 2 3 5 6 7</serverParts>
         <backgroundColor>#00000000</backgroundColor>
      </customMapSource>
   </layers>
</customMultiLayerMapSource>